<h1 align="center">Listing Categories</h1>
<div align="center">
  <%= will_paginate %>
  <% @categories.each do|category|  %>
  <div class="row">
    <div class="col-sm-6 offset-md-3">
        <div class = "card card-body bg-light">
          <div class ="article_title">
            <%= link_to "#{category.name}",category_path(category) %> 
          </div> 
        </div>
    </div>
  </div>
  <%end%>
  <%= will_paginate %>
</div>